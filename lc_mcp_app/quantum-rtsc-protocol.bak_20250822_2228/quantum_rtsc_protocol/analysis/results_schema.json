{\n  "$schema": "http://json-schema.org/draft-07/schema#",\n  "title": "RTSC Results Schema",\n  "type": "object",\n  "properties": {\n    "timestamp": { "type": "string", "format": "date-time" },\n    "inputs": {\n      "type": "object",\n      "properties": {\n        "omega_log_mev": { "type": "number" },\n        "lambda_eff": { "type": "number" },\n        "mu_star": { "type": "number" },\n        "f_omega": { "type": "number" }\n      },\n      "required": ["omega_log_mev","lambda_eff","mu_star","f_omega"]\n    },\n    "tc_estimate_K": { "type": "number" },\n    "tc_ci_K": {\n      "type": "object",\n      "properties": {\n        "p16": { "type": "number" },\n        "p50": { "type": "number" },\n        "p84": { "type": "number" }\n      },\n      "required": ["p16","p50","p84"]\n    },\n    "success_probability": {\n      "type": "object",\n      "properties": {\n        "rtsc_300K": { "type": "number" }\n      },\n      "required": ["rtsc_300K"]\n    },\n    "risk_factors": { "type": "array", "items": { "type": "string" } },\n    "optimization": {\n      "type": "object",\n      "properties": {\n        "top_levers": { "type": "array", "items": { "type": "string" } }\n      },\n      "required": ["top_levers"]\n    }\n  },\n  "required": ["timestamp","inputs","tc_estimate_K","tc_ci_K","success_probability","risk_factors","optimization"]\n}
